<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Julian portfolio", sticy: false) %>
<% eslif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Julian portfolio", sticy: false) %>
<% else flash[:notice] %>
  <%= js add_gritter(flash[:notice], title: "Julian portfolio", sticy: false) %>
<% end %>